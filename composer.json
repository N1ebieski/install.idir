{
    "name": "n1ebieski/install.idir",
    "type": "project",
    "description": "iDir installer.",
    "keywords": [
        "idir install",
        "idir installer"
    ],
    "require": {
        "php": "^7.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.20.12",
        "laravel/tinker": "^2.0",
        "n1ebieski/idir": "^4.0.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "N1ebieski\\ICore\\Seeds\\": "database/seeds/vendor/icore/",
            "N1ebieski\\IDir\\Seeds\\": "database/seeds/vendor/idir/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/N1ebieski/laravel-jsvalidation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/N1ebieski/laravel-file-manager"
        },        
        {
            "type": "vcs",
            "url": "https://github.com/N1ebieski/taggedFileCache.git"
        }        
    ],    
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
